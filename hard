cflznv=`env | grep '__.*'`
eval "which() { printf '$cflznv' | grep '__.*=/.*$1' | sed -r 's/.*=(.*)/\1/'; }"

env() { false; }

command() { eval "$@"; }
export() { false; }
unset() { false; }
